{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable",
        "content-type": "application/javascript; charset=utf-8"
      },
      "continue": true
    },
    {
      "src": "/(.*)\\.js$",
      "headers": {
        "content-type": "application/javascript; charset=utf-8"
      },
      "continue": true
    },
    {
      "src": "/(.*)\\.css$",
      "headers": {
        "content-type": "text/css; charset=utf-8"
      },
      "continue": true
    },
    {
      "src": "/(.*)\\.json$",
      "headers": {
        "content-type": "application/json; charset=utf-8"
      },
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 